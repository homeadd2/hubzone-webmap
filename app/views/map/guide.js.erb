$('body').prepend("<%= j render partial: 'map_guide' %>");

$('.usa-overlay').addClass('is-visible');
$('.usa-nav').removeClass('is-visible');

$('button.usa-button.close').click(function() {
  $('.usa-modal').remove();
  $('.usa-overlay').removeClass('is-visible');
});

$('.usa-modal').click(function(e) {
    if (e.target !== this) {
      return;
    } else {
      $('.usa-modal').remove();
      $('.usa-overlay').removeClass('is-visible');
    }
});
